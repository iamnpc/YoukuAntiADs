{
    "manifest_version": 2,
    "description":  "开源的乐视，优酷，土豆，爱奇艺，酷六去广告插件（轻量版）",
    "name":         "Green.Player.Lite",
    "version":      "2.1.1009.1",
    "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon.png"
    },
    "permissions": [
		"activeTab",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
    "browser_action": {
    	"default_icon": "icon.png",
		"default_popup": "popup.html"
    },
    "background": {
		"page" : "background.html"
	}
	
}