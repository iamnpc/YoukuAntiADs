{
    "manifest_version": 2,
    "description":  "Youku Tudou iQiyi Letv Ku6 remove ads",
    "name":         "Youku Anti ADs",
    "version":      "2.0.1008",
    "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon.png"
    },
    "permissions": [
		"activeTab",
		"tabs",
		"http://*/*",
		"https://*/*"
	],
    "browser_action": {
    	"default_icon": "icon.png",
		"default_popup": "popup.html"
    },
    "background": {
		"page" : "background.html"
	}
	
}